	.Q7(Q7),
	.RA1(RA1),
	.RA2(RA2),
	.t0(t0),
	.t1(t1),
	.WA(WA)
);

// clk
always
begin
	clk = 1'b0;
	clk = #20000 1'b1;
	#20000;
end 
